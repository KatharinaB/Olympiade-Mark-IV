package ui.PlayerView.Elements;

import javax.swing.JLabel;

/**
 * Hält das Label "Team" und dazugehörigen Wert
 * @author Katy
 *
 */
public class PlayerteamNameLabel extends JLabel{

	private JLabel teamLabel;
	private JLabel teamName;
	
	public PlayerteamNameLabel() {
		this.setBounds(350,50,100,20);
		
		teamLabel = new JLabel("Team:");
		teamName = new JLabel();
		
		setTeamName();
	}

	private void setTeamName() {
		teamName.setText("dummteam");//TODO aus DB
		
	}

}
