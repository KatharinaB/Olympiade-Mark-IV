package gameContent;

public class Team {
	
	private String name;
	private String shortName;
	private int id; //TODO schauen ob wir das wirklich brauchen
	private int salary;
	private int rank;
	private String captain;
	

	public Team(String name, String shortName, int id, int salary, int rank, String captain) {
		this.name = name;
		this.shortName = shortName;
		this.id = id;
		this.salary = salary;
		this.rank = rank;
		this.captain = captain;
	}

}
