import javax.swing.JPanel;
import javax.swing.JTextArea;


public class TickerPanel extends JPanel{
	
	private JTextArea newsticker;
	private JTextArea transferticker;
	
	public TickerPanel(){
		
		initTransferticker();
		
		this.add(newsticker);
		this.add(transferticker);
	}
	
	private void initTransferticker() {
		transferticker = new JTextArea("newsticker (Anzeige der letzten News");
		transferticker.setBounds(1010, 100, 250, 100);
		
	}


}
